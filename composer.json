{
    "name": "apsg/da-gallery-fetcher",
    "description": "deviantArt Gallery fetcher",
    "keywords": [
        "apsg",
        "da-gallery-fetcher"
    ],
    "homepage": "https://github.com/apsg/da-gallery-fetcher",
    "license": "MIT",
    "authors": [
        {
            "name": "Szymon Gackowski",
            "email": "szymon@gackowski.edu.pl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-xml": "*",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^8.0",
        "laravel/framework": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Apsg\\Dafetcher\\": "src",
            "Apsg\\Dafetcher\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apsg\\Dafetcher\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DafetcherServiceProvider"
            ],
            "aliases": {
                "Dafetcher": "DafetcherFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
